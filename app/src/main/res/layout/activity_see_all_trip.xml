    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".alltrips.SeeAllTripActivity">

    <include
        android:id="@+id/toolbar"
        layout="@layout/toolbar_layout"/>

    <android.support.v7.widget.RecyclerView
